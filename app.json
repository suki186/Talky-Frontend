{
  "expo": {
    "name": "Talky",
    "slug": "Talky",
    "owner": "talky-front",
    "version": "1.0.0",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/splash/splash-bg.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFEF6"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sukiii.Talky"
    },
    "android": {
      "permissions": ["RECORD_AUDIO"],
      "edgeToEdgeEnabled": true,
      "package": "com.sukiii.Talky"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b1ecf1da-b65c-4c46-8106-63b73c6184ee"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/b1ecf1da-b65c-4c46-8106-63b73c6184ee"
    },
    "plugins": [
      [
        "expo-audio",
        {
          "microphonePermission": "음성 녹음을 위해 마이크 접근이 필요합니다."
        }
      ]
    ]
  }
}
